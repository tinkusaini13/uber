@Library('tinku_cloud_lib') _

pipeline {
    agent any
    
    stages {
        stage('Git Checkout'){
            steps{
            gitCheckout(
                branch: "main",
                credentialsId: 'git_token',
                url: "https://github.com/tinkusaini13/uber.git"
            )
            }
        }

        stage('Maven Build : maven'){
           steps{
               script{      
                   mvnBuild()
               }
            }
        }

        stage('Maven Deploy'){
           steps{
               script{      
                   deployWithSSH()
               }
            }
        }
    }
}
